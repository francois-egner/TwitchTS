  "compilerOptions": {
    "strict": true,
    "outDir": "./build",
    "skipLibCheck": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": false
  },
  "files": [
    "src/index.ts"
  ],
  "include": ["src"],
  "exclude": ["node_modules","build"],
  "module": "es2022",
  "target": "es2022"
}